    "angular-ui-router": "^0.3.1",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "mongoose": "^4.4.6",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0"
  },
  "main": "index.js",
