  "scripts": {
    "build:dev": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "node --max-old-space-size=4096 ./node_modules/webpack/bin/webpack --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "build": "npm run build:dev",
    "ci": "npm run e2e && npm run test",
    "clean:all": "npm cache clean",
    "clean:dist": "rimraf dist",
    "clean:install": "npm install --no-optional",
    "clean:start": "npm start",
    "clean": "rimraf reports dist",
    "lint": "tsconfig-lint .",
    "prebuild:dev": "npm run clean",
    "prebuild:prod": "npm run clean",
    "preclean:all": "npm run clean",
    "preclean:install": "npm run clean:all && rimraf node_modules",
    "preclean:start": "npm run clean",
    "preserver:dev": "npm run clean",
    "serve-api": "nodemon index.js",
    "server:dev:livereload": "npm run server:dev -- --inline",
    "server:dev": "node --max-old-space-size=4096 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config config/webpack.dev.js --colors --watch --open",
    "server:prod": "npm run superstatic",
    "server": "npm run server:dev:livereload",
    "start:dev": "npm run server",
    "start:livereload": "npm run server:dev:livereload",
    "start:prod": "npm run server:prod",
    "start": "npm run server",
    "test:watch": "jest --watch",
    "test": "jest",
    "validate-webpack": "webpack-validator webpack.config.js"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "html"
    ],
    "moduleNameMapper": {
      "common/(.*)": "<rootDir>/src/app/common/$1",
      "core/(.*)": "<rootDir>/src/app/core/$1"
    },
    "rootDir": ".",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "(html)": "<rootDir>/jest-templatecache-transformer.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "verbose": true
  }
